<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" android:drawable="@color/list_pressed_solid"/>
    <item android:state_focused="true" android:state_checked="true">
        <layer-list>
            <item android:drawable="@drawable/btn_compound_checkmark"/>
            <item android:drawable="@drawable/list_focused"/>
        </layer-list>
    </item>
    <item android:state_focused="true" android:drawable="@drawable/list_focused"/>
    <item android:state_checked="true" android:drawable="@drawable/btn_compound_checkmark"/>
    <item android:drawable="@android:color/transparent"/>
</selector>
